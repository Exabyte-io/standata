          },
          "type": "io"
        },
        {
          "config": {
            "flowchartId": "end-of-ml-train-head",
            "name": "End Setup",
            "operand": "IS_SETUP_COMPLETE",
            "value": "True"
          },
          "type": "assignment"
        }
      ]
